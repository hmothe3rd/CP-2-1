#include <iostream>
#include <sstream>
#include <limits>

using namespace std;

float Add2Values(float, float);

int main()
{
    char x, y;

    cout << "-- Add 2 Values --\n"
         << "Enter x Value: ";

    cin >> x;

    cout << "Enter y Value: ";
    
    cin >> y;

    cout << "__________________\n";

    return 0;
}

float Add2Values(float x, float y)
{
    float sum = x + y;

    return sum;
}
